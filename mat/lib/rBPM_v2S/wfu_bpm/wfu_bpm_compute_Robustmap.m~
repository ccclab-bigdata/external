function [Romap] = wfu_bpm_compute_Robustmap(Pvalue,c,brain_mask,Romap,M,N)
%---------- computing Fmaps for regression ------------------%
for m = 1:M
    for n = 1:N
        if brain_mask(m,n) == 1
                p_val = Pvalue(m,n,:);
                p_val(1) = [];
                Romap(m,n) = c'*p_val;
        end
    end
end

